<div class="container">
  <%= form_for @catelogy do |f|%>
  
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
  
    <div class="field">
      <%= f.label :description %>
      <%= f.text_area :description %>
    </div>
  
    <%= f.submit %>
  <% end%>
</div>
